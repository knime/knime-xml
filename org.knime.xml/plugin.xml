      </node>
      <node
            category-path="/misc/xml"
            expert-flag="false"
            factory-class="org.knime.xml.node.rcombine.XMLRowCombinerNodeFactory"
            id="org.knime.xml.node.rcombine.XMLRowCombinerNodeFactory">
      </node>
      <node
            category-path="/misc/xml"
            expert-flag="false"
            factory-class="org.knime.xml.node.xslt.XSLTNodeFactory"
            id="org.knime.xml.node.xslt.XSLTNodeFactory">
      </node>      
   </extension>
   <extension
         point="org.knime.workbench.repository.categories">
      <category
            description="Nodes for processing XML"
            icon="icons/xml.png"
            level-id="xml"
            name="XML"
            path="/misc">
      </category>
